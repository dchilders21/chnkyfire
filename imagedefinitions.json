{
  "taskDefinition": {
    "family": "chnkyfire-task",
    "containerDefinitions": [
      {
        "name": "chnkyfire-container",
        "image": "157608028139.dkr.ecr.us-west-1.amazonaws.com/chnkyfire",
        "cpu": 100,
        "portMappings": [
          {
            "protocol": "tcp",
            "containerPort": 80,
            "hostPort": 80
          }
        ],
        "memory": 128,
        "essential": true
      }
    ]
  }
}